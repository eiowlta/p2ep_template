function START()
{
    controlDisable();
    ref_1 = dialogAlloc();
    ref_2 = msgShow(msg_0);
/***************************************
[color(green)]The map of Smile Hirasaka
has been completed.[color(white)]
[wait][clear]
***************************************/
    wait(ref_2);
    dialogFree(ref_1);
    bitClear(IBIT_BLK_MAP_0B);
    bitSet(IBIT_CMP_MAP_0B);
    bitClear(IBIT_BLK_MAP_2A);
    bitSet(IBIT_CMP_MAP_2A);
    controlEnable();
}
