function START()
{
    controlDisable();
    ref_1 = dialogAlloc();
    ref_2 = msgShow(msg_0);
/***************************************
[color(yellow)]Voice of Jack Frost[color(white)]
[tab]The fire... still won’t go out, HO!
[tab]But, we’re almost there, HO!
[tab]If I go...
[wait][clear]
***************************************/
    wait(ref_2);
    dialogFree(ref_1);
    bitClear(MBIT_EXE_ZISATU4);
    controlEnable();
}
