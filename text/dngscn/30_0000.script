function START()
{
    controlDisable();
    ref_1 = dialogAlloc();
    ref_2 = msgShow(msg_0);
/***************************************
[color(green)]The map of Mt. Iwato
has been completed.[color(white)]
[wait][clear]
***************************************/
    wait(ref_2);
    dialogFree(ref_1);
    bitClear(IBIT_BLK_MAP_0F);
    bitSet(IBIT_CMP_MAP_0F);
    controlEnable();
}
